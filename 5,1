#include <stdio.h>
#include <math.h>
int main() {
    float x,j,i,sum;
    printf("Enter value of x :");
    scanf("%f",&x);

    for (i=0;i<=5;i++){
    j=i*2;
    
    int f=1,k;
    for(k=1;k<=j;k++){
        f*=k;
    }
   sum+=((pow(-1,j))*(pow(x,j))/(f));
  
}
printf("%.1f",sum);
    return 0;
}
